{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "leaflet-map-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/leafletMap/LeafletMapWebPart.js",
          "manifest": "./src/webparts/leafletMap/LeafletMapWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "leaflet":"node_modules/leaflet/dist/leaflet.js",
    "jquery":"node_modules/jquery/dist/jquery.min.js",
    "jqueryui":"node_modules/jqueryui/jquery-ui.min.js"
  },
  "localizedResources": {
    "LeafletMapWebPartStrings": "lib/webparts/leafletMap/loc/{locale}.js"
  }
}
